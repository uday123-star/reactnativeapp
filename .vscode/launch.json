  "version": "0.2.0",
  "configurations": [
    {
      "name": "Expo - attach to simulator",
      "port": 8081,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**/*",
      ],
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "sourceMaps": true,
    },
    {
      "name": "Expo - Classmates Debug",
      "request": "launch",
      "type": "reactnative",
      "cwd": "${workspaceFolder}",
      "platform": "exponent",
      "expoHostType": "lan",
      "skipFiles": [
        "node_modules/**/*",
      ],
    }
  ]
}